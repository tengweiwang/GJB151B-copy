<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ManageSysPdfEntity" table="manage_sys_pdf" schema="gjb151b">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="title">
            <column name="title" sql-type="mediumtext" not-null="false"/>
        </property>
        <property name="content">
            <column name="content" sql-type="mediumtext" not-null="false"/>
        </property>
        <property name="picture">
            <column name="picture" sql-type="mediumtext" not-null="false"/>
        </property>
        <property name="startPageNumber">
            <column name="start_page_number" sql-type="mediumtext" not-null="true"/>
        </property>
        <property name="endPageNumber">
            <column name="end_page_number" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>